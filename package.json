{
  "name": "renuel",
  "description": "Build React UIs without JSX. Type-safe, composable, and 100% JavaScript/TypeScript.",
  "version": "0.0.18-next.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/renueljs/renuel.git"
  },
  "homepage": "https://github.com/renueljs/renuel#readme",
  "bugs": {
    "url": "https://github.com/renueljs/renuel/issues"
  },
  "keywords": [
    "react",
    "jsx",
    "typescript",
    "ui",
    "components",
    "type-safe",
    "no-jsx",
    "hyperscript",
    "vanilla"
  ],
  "type": "module",
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@eslint/js": "^9.34.0",
    "@testing-library/react": "^16.3.0",
    "@types/react": "^18.3.0",
    "@types/semver": "^7.7.1",
    "@typescript-eslint/eslint-plugin": "^8.40.0",
    "eslint": "^9.34.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-unused-imports": "^4.2.0",
    "global-jsdom": "^26.0.0",
    "husky": "^8.0.0",
    "prettier": "^3.6.2",
    "react": "^18.0.0",
    "typescript": "~5.8",
    "typescript-eslint": "^8.40.0"
  },
  "peerDependencies": {
    "@types/react": ">=18.3.0",
    "react": ">=18.0.0"
  },
  "peerDependenciesMeta": {
    "@types/react": {
      "optional": true
    }
  },
  "scripts": {
    "prepare": "husky install",
    "prepublishOnly": "tsc --project ./tsconfig.build.json --outDir dist/cjs --module CommonJS && tsc --project ./tsconfig.build.json --outDir dist/esm --module ESNext && tsc --project ./tsconfig.build.json --outDir dist/types --emitDeclarationOnly",
    "test": "tsc --project tsconfig.test.json && node src/index.spec.ts && tsc --project tsconfig.script.json",
    "test-watch": "node --watch src/index.spec.ts"
  },
  "files": [
    "dist"
  ],
  "module": "./dist/esm/index.js",
  "main": "./dist/cjs/index.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "types": "./dist/types/index.d.ts"
    }
  }
}
